{
  "name": "@typespecs/elixir-ast",
  "version": "0.1.0",
  "description": "TypeSpec definitions for Elixir AST",
  "license": "MIT",
  "type": "module",
  "repository": {
    "url": "https://github.com/typespecs/elixir-ast"
  },
  "author": {
    "name": "Stephan Meijer",
    "email": "typespecs@stephanmeijer.com"
  },
  "exports": {
    ".": {
      "typespec": "./lib/main.tsp"
    }
  },
  "scripts": {
    "build": "tsp compile lib/.",
    "watch": "tsp compile lib/. --watch",
    "format": "tsp format lib/**/*.tsp"
  },
  "peerDependencies": {
    "@typespec/compiler": "~1.9.0"
  },
  "dependencies": {
    "@typespec/json-schema": "~1.9.0"
  }
}
